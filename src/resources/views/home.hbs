<div class="row">
  {{#each courses}}
  <div class="card">
    <img class="card-img-top" src="{{this.image}}" alt="Card image cap">
    <div class="card-body">
      <hf5 class="card-title">{{this.name}}</h5>
      <p class="card-text">{{this.description}}</p>
      <p class="card-text"><small class="text-muted">{{this.detail}}</small></p>
    </div>
  </div>
  {{/each}}
</div> 